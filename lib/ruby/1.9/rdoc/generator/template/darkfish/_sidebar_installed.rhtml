<nav id="home-section" class="section">
  <h3 class="section-header">Documentation</h3>

  <ul>
  <% installed.each do |name, href, exists, type, _| %>
    <% next if type == :extra %>
    <li class="folder">
    <% if exists then %>
      <a href="<%= href %>"><%= h name %></a>
    <% else %>
      <%= h name %>
    <% end %>
  <% end %>
  </ul>
</nav>
